{
  "name": "express-elastic-sanitize",
  "version": "0.0.1",
  "description": "Elasticsearch input sanitizer for Express",
  "main": "build/index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --require babel-polyfill --recursive",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "./node_modules/babel-cli/bin/babel.js src -d build",
    "prePublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riipen/express-elastic-sanitize.git"
  },
  "keywords": [
    "Elasticsearch",
    "Query",
    "Sanitize",
    "Express"
  ],
  "author": "Jordan Ell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/riipen/express-elastic-sanitize/issues"
  },
  "homepage": "https://github.com/riipen/express-elastic-sanitize#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.0",
    "eslint": "4.2.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "mocha": "3.4.2"
  }
}
